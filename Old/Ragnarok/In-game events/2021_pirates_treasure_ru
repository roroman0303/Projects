npc "ayo_fild01" "#ayo_fild01_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#ayo_fild01_1"
	disablenpc "Сундук#ayo_fild01_1"
	disablenpc "#Aura_ayo_fild01_1"
	disablenpc "Метка#ayo_fild01_2"
	disablenpc "Сундук#ayo_fild01_2"
	disablenpc "#Aura_ayo_fild01_2"
	disablenpc "Метка#ayo_fild01_3"
	disablenpc "Сундук#ayo_fild01_3"
	disablenpc "#Aura_ayo_fild01_3"
	cmdothernpc "#ayo_fild01_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ayo_fild01_1"
	elseif (place == 2)
		enablenpc "Метка#ayo_fild01_2"
	else
		enablenpc "Метка#ayo_fild01_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ayo_fild01_1"
	disablenpc "Метка#ayo_fild01_2"
	disablenpc "Метка#ayo_fild01_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#ayo_fild01_1"
	disablenpc "#Aura_ayo_fild01_1"
	disablenpc "Сундук#ayo_fild01_2"
