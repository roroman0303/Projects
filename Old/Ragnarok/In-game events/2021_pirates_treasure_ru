OnInit:
	disablenpc "Метка#ra_fild03_1"
	disablenpc "Сундук#ra_fild03_1"
	disablenpc "#Aura_ra_fild03_1"
	disablenpc "Метка#ra_fild03_2"
	disablenpc "Сундук#ra_fild03_2"
	disablenpc "#Aura_ra_fild03_2"
	disablenpc "Метка#ra_fild03_3"
	disablenpc "Сундук#ra_fild03_3"
	disablenpc "#Aura_ra_fild03_3"
	cmdothernpc "#ra_fild03_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ra_fild03_1"
	elseif (place == 2)
		enablenpc "Метка#ra_fild03_2"
	else
		enablenpc "Метка#ra_fild03_3"
	endif
return
