OnInit:
	disablenpc "Метка#moc_fild07_1"
	disablenpc "Сундук#moc_fild07_1"
	disablenpc "#Aura_moc_fild07_1"
	disablenpc "Метка#moc_fild07_2"
	disablenpc "Сундук#moc_fild07_2"
	disablenpc "#Aura_moc_fild07_2"
	disablenpc "Метка#moc_fild07_3"
	disablenpc "Сундук#moc_fild07_3"
	disablenpc "#Aura_moc_fild07_3"
	cmdothernpc "#moc_fild07_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#moc_fild07_1"
	elseif (place == 2)
		enablenpc "Метка#moc_fild07_2"
	else
		enablenpc "Метка#moc_fild07_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#moc_fild07_1"
	disablenpc "Метка#moc_fild07_2"
	disablenpc "Метка#moc_fild07_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#moc_fild07_1"
