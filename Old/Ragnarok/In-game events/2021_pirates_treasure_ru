npc "pay_fild07" "#pay_fild07_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#pay_fild07_1"
	disablenpc "Сундук#pay_fild07_1"
	disablenpc "#Aura_pay_fild07_1"
	disablenpc "Метка#pay_fild07_2"
	disablenpc "Сундук#pay_fild07_2"
	disablenpc "#Aura_pay_fild07_2"
	disablenpc "Метка#pay_fild07_3"
	disablenpc "Сундук#pay_fild07_3"
	disablenpc "#Aura_pay_fild07_3"
	cmdothernpc "#pay_fild07_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#pay_fild07_1"
	elseif (place == 2)
		enablenpc "Метка#pay_fild07_2"
	else
		enablenpc "Метка#pay_fild07_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#pay_fild07_1"
	disablenpc "Метка#pay_fild07_2"
	disablenpc "Метка#pay_fild07_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#pay_fild07_1"
	disablenpc "#Aura_pay_fild07_1"
	disablenpc "Сундук#pay_fild07_2"
	disablenpc "#Aura_pay_fild07_2"
	disablenpc "Сундук#pay_fild07_3"
	disablenpc "#Aura_pay_fild07_3"
return
OnTimer: 600000
	cmdothernpc "#pay_fild07_control" "spawn"
	stoptimer
return
