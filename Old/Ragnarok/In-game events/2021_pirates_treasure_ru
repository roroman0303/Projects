npc "gef_fild09" "#gef_fild09_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#gef_fild09_1"
	disablenpc "Сундук#gef_fild09_1"
	disablenpc "#Aura_gef_fild09_1"
	disablenpc "Метка#gef_fild09_2"
	disablenpc "Сундук#gef_fild09_2"
	disablenpc "#Aura_gef_fild09_2"
	disablenpc "Метка#gef_fild09_3"
	disablenpc "Сундук#gef_fild09_3"
	disablenpc "#Aura_gef_fild09_3"
	cmdothernpc "#gef_fild09_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#gef_fild09_1"
	elseif (place == 2)
		enablenpc "Метка#gef_fild09_2"
	else
		enablenpc "Метка#gef_fild09_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#gef_fild09_1"
	disablenpc "Метка#gef_fild09_2"
	disablenpc "Метка#gef_fild09_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#gef_fild09_1"
	disablenpc "#Aura_gef_fild09_1"
	disablenpc "Сундук#gef_fild09_2"
	disablenpc "#Aura_gef_fild09_2"
	disablenpc "Сундук#gef_fild09_3"
	disablenpc "#Aura_gef_fild09_3"
return
OnTimer: 600000
	cmdothernpc "#gef_fild09_control" "spawn"
	stoptimer
return
