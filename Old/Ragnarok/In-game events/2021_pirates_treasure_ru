npc "ein_fild05" "#ein_fild05_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#ein_fild05_1"
	disablenpc "Сундук#ein_fild05_1"
	disablenpc "#Aura_ein_fild05_1"
	disablenpc "Метка#ein_fild05_2"
	disablenpc "Сундук#ein_fild05_2"
	disablenpc "#Aura_ein_fild05_2"
	disablenpc "Метка#ein_fild05_3"
	disablenpc "Сундук#ein_fild05_3"
	disablenpc "#Aura_ein_fild05_3"
	cmdothernpc "#ein_fild05_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ein_fild05_1"
	elseif (place == 2)
		enablenpc "Метка#ein_fild05_2"
	else
		enablenpc "Метка#ein_fild05_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ein_fild05_1"
	disablenpc "Метка#ein_fild05_2"
	disablenpc "Метка#ein_fild05_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#ein_fild05_1"
	disablenpc "#Aura_ein_fild05_1"
	disablenpc "Сундук#ein_fild05_2"
	disablenpc "#Aura_ein_fild05_2"
	disablenpc "Сундук#ein_fild05_3"
	disablenpc "#Aura_ein_fild05_3"
return
OnTimer: 600000
	cmdothernpc "#ein_fild05_control" "spawn"
	stoptimer
return
