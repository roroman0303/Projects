npc "bra_fild01" "#bra_fild01_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#bra_fild01_1"
	disablenpc "Сундук#bra_fild01_1"
	disablenpc "#Aura_bra_fild01_1"
	disablenpc "Метка#bra_fild01_2"
	disablenpc "Сундук#bra_fild01_2"
	disablenpc "#Aura_bra_fild01_2"
	disablenpc "Метка#bra_fild01_3"
	disablenpc "Сундук#bra_fild01_3"
	disablenpc "#Aura_bra_fild01_3"
	cmdothernpc "#bra_fild01_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#bra_fild01_1"
	elseif (place == 2)
		enablenpc "Метка#bra_fild01_2"
	else
		enablenpc "Метка#bra_fild01_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#bra_fild01_1"
	disablenpc "Метка#bra_fild01_2"
	disablenpc "Метка#bra_fild01_3"
	InitTimer
return
