npc "mjolnir_09" "#mjolnir_09_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#mjolnir_09_1"
	disablenpc "Сундук#mjolnir_09_1"
	disablenpc "#Aura_mjolnir_09_1"
	disablenpc "Метка#mjolnir_09_2"
	disablenpc "Сундук#mjolnir_09_2"
	disablenpc "#Aura_mjolnir_09_2"
	disablenpc "Метка#mjolnir_09_3"
	disablenpc "Сундук#mjolnir_09_3"
	disablenpc "#Aura_mjolnir_09_3"
	cmdothernpc "#mjolnir_09_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#mjolnir_09_1"
	elseif (place == 2)
		enablenpc "Метка#mjolnir_09_2"
	else
		enablenpc "Метка#mjolnir_09_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#mjolnir_09_1"
	disablenpc "Метка#mjolnir_09_2"
	disablenpc "Метка#mjolnir_09_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#mjolnir_09_1"
	disablenpc "#Aura_mjolnir_09_1"
	disablenpc "Сундук#mjolnir_09_2"
	disablenpc "#Aura_mjolnir_09_2"
	disablenpc "Сундук#mjolnir_09_3"
	disablenpc "#Aura_mjolnir_09_3"
return
OnTimer: 600000
	cmdothernpc "#mjolnir_09_control" "spawn"
	stoptimer
return
