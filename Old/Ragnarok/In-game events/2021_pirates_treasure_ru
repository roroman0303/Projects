OnInit:
	disablenpc "Метка#ein_fild08_1"
	disablenpc "Сундук#ein_fild08_1"
	disablenpc "#Aura_ein_fild08_1"
	disablenpc "Метка#ein_fild08_2"
	disablenpc "Сундук#ein_fild08_2"
	disablenpc "#Aura_ein_fild08_2"
	disablenpc "Метка#ein_fild08_3"
	disablenpc "Сундук#ein_fild08_3"
	disablenpc "#Aura_ein_fild08_3"
	cmdothernpc "#ein_fild08_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ein_fild08_1"
	elseif (place == 2)
		enablenpc "Метка#ein_fild08_2"
	else
		enablenpc "Метка#ein_fild08_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ein_fild08_1"
	disablenpc "Метка#ein_fild08_2"
	disablenpc "Метка#ein_fild08_3"
	InitTimer
return
