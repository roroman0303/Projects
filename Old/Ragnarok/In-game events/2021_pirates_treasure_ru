OnInit:
	disablenpc "Метка#ecl_fild01_1"
	disablenpc "Сундук#ecl_fild01_1"
	disablenpc "#Aura_ecl_fild01_1"
	disablenpc "Метка#ecl_fild01_2"
	disablenpc "Сундук#ecl_fild01_2"
	disablenpc "#Aura_ecl_fild01_2"
	disablenpc "Метка#ecl_fild01_3"
	disablenpc "Сундук#ecl_fild01_3"
	disablenpc "#Aura_ecl_fild01_3"
	cmdothernpc "#ecl_fild01_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ecl_fild01_1"
	elseif (place == 2)
		enablenpc "Метка#ecl_fild01_2"
	else
		enablenpc "Метка#ecl_fild01_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ecl_fild01_1"
	disablenpc "Метка#ecl_fild01_2"
	disablenpc "Метка#ecl_fild01_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#ecl_fild01_1"
	disablenpc "#Aura_ecl_fild01_1"
	disablenpc "Сундук#ecl_fild01_2"
	disablenpc "#Aura_ecl_fild01_2"
	disablenpc "Сундук#ecl_fild01_3"
	disablenpc "#Aura_ecl_fild01_3"
return
OnTimer: 600000
	cmdothernpc "#ecl_fild01_control" "spawn"
	stoptimer
return
