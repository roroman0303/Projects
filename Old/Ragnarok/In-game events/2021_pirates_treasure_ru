OnInit:
	disablenpc "Метка#um_fild04_1"
	disablenpc "Сундук#um_fild04_1"
	disablenpc "#Aura_um_fild04_1"
	disablenpc "Метка#um_fild04_2"
	disablenpc "Сундук#um_fild04_2"
	disablenpc "#Aura_um_fild04_2"
	disablenpc "Метка#um_fild04_3"
	disablenpc "Сундук#um_fild04_3"
	disablenpc "#Aura_um_fild04_3"
	cmdothernpc "#um_fild04_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#um_fild04_1"
	elseif (place == 2)
		enablenpc "Метка#um_fild04_2"
	else
		enablenpc "Метка#um_fild04_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#um_fild04_1"
	disablenpc "Метка#um_fild04_2"
	disablenpc "Метка#um_fild04_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#um_fild04_1"
	disablenpc "#Aura_um_fild04_1"
	disablenpc "Сундук#um_fild04_2"
	disablenpc "#Aura_um_fild04_2"
	disablenpc "Сундук#um_fild04_3"
	disablenpc "#Aura_um_fild04_3"
return
OnTimer: 600000
	cmdothernpc "#um_fild04_control" "spawn"
	stoptimer
