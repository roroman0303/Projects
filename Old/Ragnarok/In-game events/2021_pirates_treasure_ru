npc "pay_fild10" "#pay_fild10_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#pay_fild10_1"
	disablenpc "Сундук#pay_fild10_1"
	disablenpc "#Aura_pay_fild10_1"
	disablenpc "Метка#pay_fild10_2"
	disablenpc "Сундук#pay_fild10_2"
	disablenpc "#Aura_pay_fild10_2"
	disablenpc "Метка#pay_fild10_3"
	disablenpc "Сундук#pay_fild10_3"
	disablenpc "#Aura_pay_fild10_3"
	cmdothernpc "#pay_fild10_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#pay_fild10_1"
	elseif (place == 2)
		enablenpc "Метка#pay_fild10_2"
	else
		enablenpc "Метка#pay_fild10_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#pay_fild10_1"
	disablenpc "Метка#pay_fild10_2"
	disablenpc "Метка#pay_fild10_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#pay_fild10_1"
	disablenpc "#Aura_pay_fild10_1"
	disablenpc "Сундук#pay_fild10_2"
	disablenpc "#Aura_pay_fild10_2"
	disablenpc "Сундук#pay_fild10_3"
	disablenpc "#Aura_pay_fild10_3"
return
OnTimer: 600000
	cmdothernpc "#pay_fild10_control" "spawn"
	stoptimer
return
