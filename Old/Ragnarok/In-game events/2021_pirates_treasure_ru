OnInit:
	disablenpc "Метка#prt_fild02_1"
	disablenpc "Сундук#prt_fild02_1"
	disablenpc "#Aura_prt_fild02_1"
	disablenpc "Метка#prt_fild02_2"
	disablenpc "Сундук#prt_fild02_2"
	disablenpc "#Aura_prt_fild02_2"
	disablenpc "Метка#prt_fild02_3"
	disablenpc "Сундук#prt_fild02_3"
	disablenpc "#Aura_prt_fild02_3"
	cmdothernpc "#prt_fild02_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#prt_fild02_1"
	elseif (place == 2)
		enablenpc "Метка#prt_fild02_2"
	else
		enablenpc "Метка#prt_fild02_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#prt_fild02_1"
	disablenpc "Метка#prt_fild02_2"
	disablenpc "Метка#prt_fild02_3"
	InitTimer
return
