npc "um_fild01" "#um_fild01_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#um_fild01_1"
	disablenpc "Сундук#um_fild01_1"
	disablenpc "#Aura_um_fild01_1"
	disablenpc "Метка#um_fild01_2"
	disablenpc "Сундук#um_fild01_2"
	disablenpc "#Aura_um_fild01_2"
	disablenpc "Метка#um_fild01_3"
	disablenpc "Сундук#um_fild01_3"
	disablenpc "#Aura_um_fild01_3"
	cmdothernpc "#um_fild01_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#um_fild01_1"
	elseif (place == 2)
		enablenpc "Метка#um_fild01_2"
	else
		enablenpc "Метка#um_fild01_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#um_fild01_1"
	disablenpc "Метка#um_fild01_2"
	disablenpc "Метка#um_fild01_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#um_fild01_1"
	disablenpc "#Aura_um_fild01_1"
	disablenpc "Сундук#um_fild01_2"
	disablenpc "#Aura_um_fild01_2"
	disablenpc "Сундук#um_fild01_3"
	disablenpc "#Aura_um_fild01_3"
return
OnTimer: 600000
	cmdothernpc "#um_fild01_control" "spawn"
	stoptimer
return
