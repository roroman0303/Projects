	disablenpc "Сундук#moc_fild18_3"
	disablenpc "#Aura_moc_fild18_3"
	cmdothernpc "#moc_fild18_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#moc_fild18_1"
	elseif (place == 2)
		enablenpc "Метка#moc_fild18_2"
	else
		enablenpc "Метка#moc_fild18_3"
	endif
return
