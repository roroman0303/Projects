npc "ama_fild01" "Метка#ama_fild01_3" 4_CRACK 155 104 5 0 0
OnClick:
	var pirate_17410 = isbegin_quest 17410
	if (pirate_17410 == 1)
	    setquest 17411
		completequest 17410
	endif
	var found = rand 1 100
	if (found <= 30)
		enablenpc "#Aura_ama_fild01_3"
		ConsumeSpecialItem Anodyne_B
	elseif (found <= 60)
		cmdothernpc "Метка#ama_fild01_3" "zombie"
	else
		cmdothernpc "Метка#ama_fild01_3" "treasure"
	endif
	cmdothernpc "#ama_fild01_control" "found"
