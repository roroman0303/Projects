npc "ra_fild06" "#ra_fild06_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#ra_fild06_1"
	disablenpc "Сундук#ra_fild06_1"
	disablenpc "#Aura_ra_fild06_1"
	disablenpc "Метка#ra_fild06_2"
	disablenpc "Сундук#ra_fild06_2"
	disablenpc "#Aura_ra_fild06_2"
	disablenpc "Метка#ra_fild06_3"
	disablenpc "Сундук#ra_fild06_3"
	disablenpc "#Aura_ra_fild06_3"
	cmdothernpc "#ra_fild06_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ra_fild06_1"
	elseif (place == 2)
		enablenpc "Метка#ra_fild06_2"
	else
		enablenpc "Метка#ra_fild06_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ra_fild06_1"
	disablenpc "Метка#ra_fild06_2"
	disablenpc "Метка#ra_fild06_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#ra_fild06_1"
	disablenpc "#Aura_ra_fild06_1"
	disablenpc "Сундук#ra_fild06_2"
	disablenpc "#Aura_ra_fild06_2"
	disablenpc "Сундук#ra_fild06_3"
	disablenpc "#Aura_ra_fild06_3"
return
OnTimer: 600000
	cmdothernpc "#ra_fild06_control" "spawn"
	stoptimer
return
