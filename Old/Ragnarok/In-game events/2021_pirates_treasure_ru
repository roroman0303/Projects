OnInit:
	disablenpc "Метка#ve_fild03_1"
	disablenpc "Сундук#ve_fild03_1"
	disablenpc "#Aura_ve_fild03_1"
	disablenpc "Метка#ve_fild03_2"
	disablenpc "Сундук#ve_fild03_2"
	disablenpc "#Aura_ve_fild03_2"
	disablenpc "Метка#ve_fild03_3"
	disablenpc "Сундук#ve_fild03_3"
	disablenpc "#Aura_ve_fild03_3"
	cmdothernpc "#ve_fild03_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ve_fild03_1"
	elseif (place == 2)
		enablenpc "Метка#ve_fild03_2"
	else
		enablenpc "Метка#ve_fild03_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ve_fild03_1"
	disablenpc "Метка#ve_fild03_2"
	disablenpc "Метка#ve_fild03_3"
	InitTimer
return
