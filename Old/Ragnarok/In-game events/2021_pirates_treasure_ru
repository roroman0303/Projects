npc "yuno_fild12" "#yuno_fild12_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#yuno_fild12_1"
	disablenpc "Сундук#yuno_fild12_1"
	disablenpc "#Aura_yuno_fild12_1"
	disablenpc "Метка#yuno_fild12_2"
	disablenpc "Сундук#yuno_fild12_2"
	disablenpc "#Aura_yuno_fild12_2"
	disablenpc "Метка#yuno_fild12_3"
	disablenpc "Сундук#yuno_fild12_3"
	disablenpc "#Aura_yuno_fild12_3"
	cmdothernpc "#yuno_fild12_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#yuno_fild12_1"
	elseif (place == 2)
		enablenpc "Метка#yuno_fild12_2"
	else
		enablenpc "Метка#yuno_fild12_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#yuno_fild12_1"
	disablenpc "Метка#yuno_fild12_2"
	disablenpc "Метка#yuno_fild12_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#yuno_fild12_1"
	disablenpc "#Aura_yuno_fild12_1"
	disablenpc "Сундук#yuno_fild12_2"
	disablenpc "#Aura_yuno_fild12_2"
	disablenpc "Сундук#yuno_fild12_3"
	disablenpc "#Aura_yuno_fild12_3"
return
OnTimer: 600000
	cmdothernpc "#yuno_fild12_control" "spawn"
	stoptimer
return
