npc "prt_fild08" "#prt_fild08_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#prt_fild08_1"
	disablenpc "Сундук#prt_fild08_1"
	disablenpc "#Aura_prt_fild08_1"
	disablenpc "Метка#prt_fild08_2"
	disablenpc "Сундук#prt_fild08_2"
	disablenpc "#Aura_prt_fild08_2"
	disablenpc "Метка#prt_fild08_3"
	disablenpc "Сундук#prt_fild08_3"
	disablenpc "#Aura_prt_fild08_3"
	cmdothernpc "#prt_fild08_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#prt_fild08_1"
	elseif (place == 2)
		enablenpc "Метка#prt_fild08_2"
	else
		enablenpc "Метка#prt_fild08_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#prt_fild08_1"
	disablenpc "Метка#prt_fild08_2"
	disablenpc "Метка#prt_fild08_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#prt_fild08_1"
	disablenpc "#Aura_prt_fild08_1"
	disablenpc "Сундук#prt_fild08_2"
	disablenpc "#Aura_prt_fild08_2"
	disablenpc "Сундук#prt_fild08_3"
	disablenpc "#Aura_prt_fild08_3"
return
OnTimer: 600000
	cmdothernpc "#prt_fild08_control" "spawn"
	stoptimer
