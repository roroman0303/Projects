npc "ra_fild06" "Метка#ra_fild06_3" 4_CRACK 289 281 5 0 0
OnClick:
	var pirate_17410 = isbegin_quest 17410
	if (pirate_17410 == 1)
	    setquest 17411
		completequest 17410
	endif
	var found = rand 1 100
	if (found <= 30)
		enablenpc "#Aura_ra_fild06_3"
		ConsumeSpecialItem Anodyne_B
	elseif (found <= 60)
		cmdothernpc "Метка#ra_fild06_3" "zombie"
	else
		cmdothernpc "Метка#ra_fild06_3" "treasure"
	endif
	cmdothernpc "#ra_fild06_control" "found"
return	
OnCommand: "zombie"
	callmonster "ra_fild06" E_ZOMBIE_TREASURE "Душа пирата " 287 281
	callmonster "ra_fild06" E_ZOMBIE_TREASURE "Душа пирата " 291 281
	callmonster "ra_fild06" E_ZOMBIE_TREASURE "Душа пирата " 289 283
	callmonster "ra_fild06" E_ZOMBIE_TREASURE "Душа пирата " 289 279
	InitTimer
return
OnTimer: 10000
	resetmymob
	stoptimer
return
OnCommand: "treasure"
	enablenpc "Сундук#ra_fild06_3"
	var item_count = 0
	var item1 = C_Blue_Rose_Eyepatch
	var item2 = C_Choco_Minihat
	var item3 = C_Clover_Silkhat
	var item4 = C_Harvest_Festa_Hat
	var item5 = C_Gryphon_Hairband
	var item6 = Azure_Jewel
	var item7 = Cardinal_Jewel
	var item8 = Blue_Jewel
	var item9 = Golden_Jewel
	var item10 = Bluish_Green_Jewel
	var item11 = Crystal_Jewel
	var item12 = Zargon
	var item13 = Skyblue_Jewel
	var item14 = Scarlet_Jewel
	var item15 = Crystal_Jewel_
	var item16 = Acti_Potion
	var item17 = E_Small_Life_Potion
	var item18 = Giant_Fly_Wing
	var item19 = Guyak_Candy
	var item20 = E_Mysterious_Water
	var item21 = Warp_Free_Ticket
	var item22 = Gold
	var item23 = E_Med_Life_Potion
	var item24 = Comp_Tyr's_Blessing
	var item25 = Comp_Kafra_Card
	while(1)
		if (item_count < 13)
			var x = rand 282 296
			var y = rand 274 288
			var item_number = rand 1 10000
			if (item_number < 5)
				ItemDown item1 1 x y
			elseif (item_number < 9)
				ItemDown item2 1 x y
			elseif (item_number < 13)
				ItemDown item3 1 x y
			elseif (item_number < 17)
				ItemDown item4 1 x y
			elseif (item_number < 21)
				ItemDown item5 1 x y
			elseif (item_number < 521)
				ItemDown item6 1 x y
			elseif (item_number < 1021)
				ItemDown item7 1 x y
			elseif (item_number < 1521)
				ItemDown item8 1 x y
			elseif (item_number < 2021)
				ItemDown item9 1 x y
			elseif (item_number < 2521)
				ItemDown item10 1 x y
			elseif (item_number < 3021)
				ItemDown item11 1 x y
			elseif (item_number < 3521)
				ItemDown item12 1 x y
			elseif (item_number < 4021)
				ItemDown item13 1 x y
			elseif (item_number < 4521)
				ItemDown item14 1 x y
			elseif (item_number < 5001)
				ItemDown item15 1 x y
			elseif (item_number < 5501)
				ItemDown item16 1 x y
			elseif (item_number < 6001)
				ItemDown item17 1 x y
			elseif (item_number < 6501)
				ItemDown item18 1 x y
			elseif (item_number < 7001)
				ItemDown item19 1 x y
			elseif (item_number < 7501)
				ItemDown item20 1 x y
			elseif (item_number < 8001)
				ItemDown item21 1 x y
			elseif (item_number < 8501)
				ItemDown item22 1 x y
			elseif (item_number < 9001)
				ItemDown item23 1 x y
			elseif (item_number < 9501)
				ItemDown item24 1 x y
			else
				ItemDown item25 1 x y
			endif
			var item_count = item_count + 1
		else
			exitwhile
		endif
	endwhile
return

// ================[ra_fild08]======================
npc "ra_fild08" "#ra_fild08_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#ra_fild08_1"
	disablenpc "Сундук#ra_fild08_1"
	disablenpc "#Aura_ra_fild08_1"
	disablenpc "Метка#ra_fild08_2"
	disablenpc "Сундук#ra_fild08_2"
	disablenpc "#Aura_ra_fild08_2"
	disablenpc "Метка#ra_fild08_3"
	disablenpc "Сундук#ra_fild08_3"
	disablenpc "#Aura_ra_fild08_3"
	cmdothernpc "#ra_fild08_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#ra_fild08_1"
	elseif (place == 2)
		enablenpc "Метка#ra_fild08_2"
	else
		enablenpc "Метка#ra_fild08_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#ra_fild08_1"
	disablenpc "Метка#ra_fild08_2"
	disablenpc "Метка#ra_fild08_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#ra_fild08_1"
	disablenpc "#Aura_ra_fild08_1"
	disablenpc "Сундук#ra_fild08_2"
	disablenpc "#Aura_ra_fild08_2"
	disablenpc "Сундук#ra_fild08_3"
	disablenpc "#Aura_ra_fild08_3"
return
OnTimer: 600000
	cmdothernpc "#ra_fild08_control" "spawn"
	stoptimer
return
