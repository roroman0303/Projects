npc "lhz_fild03" "#lhz_fild03_control" HIDDEN_NPC 1 1 5 0 0
OnInit:
	disablenpc "Метка#lhz_fild03_1"
	disablenpc "Сундук#lhz_fild03_1"
	disablenpc "#Aura_lhz_fild03_1"
	disablenpc "Метка#lhz_fild03_2"
	disablenpc "Сундук#lhz_fild03_2"
	disablenpc "#Aura_lhz_fild03_2"
	disablenpc "Метка#lhz_fild03_3"
	disablenpc "Сундук#lhz_fild03_3"
	disablenpc "#Aura_lhz_fild03_3"
	cmdothernpc "#lhz_fild03_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#lhz_fild03_1"
	elseif (place == 2)
		enablenpc "Метка#lhz_fild03_2"
	else
		enablenpc "Метка#lhz_fild03_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#lhz_fild03_1"
	disablenpc "Метка#lhz_fild03_2"
	disablenpc "Метка#lhz_fild03_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#lhz_fild03_1"
	disablenpc "#Aura_lhz_fild03_1"
	disablenpc "Сундук#lhz_fild03_2"
	disablenpc "#Aura_lhz_fild03_2"
	disablenpc "Сундук#lhz_fild03_3"
	disablenpc "#Aura_lhz_fild03_3"
return
OnTimer: 600000
	cmdothernpc "#lhz_fild03_control" "spawn"
	stoptimer
return
