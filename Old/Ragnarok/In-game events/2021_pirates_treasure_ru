	var item22 = Gold
	var item23 = E_Med_Life_Potion
	var item24 = Comp_Tyr's_Blessing
	var item25 = Comp_Kafra_Card
	while(1)
		if (item_count < 13)
			var x = rand 330 344
			var y = rand 96 110
			var item_number = rand 1 10000
			if (item_number < 5)
				ItemDown item1 1 x y
			elseif (item_number < 9)
