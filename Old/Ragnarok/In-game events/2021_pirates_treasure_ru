	var pirate_17410 = isbegin_quest 17410
	if (pirate_17410 == 1)
	    setquest 17411
		completequest 17410
	endif
	var found = rand 1 100
	if (found <= 30)
		enablenpc "#Aura_lhz_fild02_2"
		ConsumeSpecialItem Anodyne_B
	elseif (found <= 60)
		cmdothernpc "Метка#lhz_fild02_2" "zombie"
	else
		cmdothernpc "Метка#lhz_fild02_2" "treasure"
	endif
	cmdothernpc "#lhz_fild02_control" "found"
return	
