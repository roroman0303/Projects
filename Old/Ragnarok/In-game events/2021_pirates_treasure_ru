OnInit:
	disablenpc "Метка#prt_fild05_1"
	disablenpc "Сундук#prt_fild05_1"
	disablenpc "#Aura_prt_fild05_1"
	disablenpc "Метка#prt_fild05_2"
	disablenpc "Сундук#prt_fild05_2"
	disablenpc "#Aura_prt_fild05_2"
	disablenpc "Метка#prt_fild05_3"
	disablenpc "Сундук#prt_fild05_3"
	disablenpc "#Aura_prt_fild05_3"
	cmdothernpc "#prt_fild05_control" "spawn"
return
OnCommand: "spawn"
	var place = rand 1 3
	if (place == 1)
		enablenpc "Метка#prt_fild05_1"
	elseif (place == 2)
		enablenpc "Метка#prt_fild05_2"
	else
		enablenpc "Метка#prt_fild05_3"
	endif
return
OnCommand: "found"
	disablenpc "Метка#prt_fild05_1"
	disablenpc "Метка#prt_fild05_2"
	disablenpc "Метка#prt_fild05_3"
	InitTimer
return
OnTimer: 10000
	disablenpc "Сундук#prt_fild05_1"
	disablenpc "#Aura_prt_fild05_1"
	disablenpc "Сундук#prt_fild05_2"
	disablenpc "#Aura_prt_fild05_2"
	disablenpc "Сундук#prt_fild05_3"
	disablenpc "#Aura_prt_fild05_3"
return
