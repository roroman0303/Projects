	disablenpc "#Aura_cmd_fild04_2"
	disablenpc "Сундук#cmd_fild04_3"
	disablenpc "#Aura_cmd_fild04_3"
return
OnTimer: 600000
	cmdothernpc "#cmd_fild04_control" "spawn"
	stoptimer
return
